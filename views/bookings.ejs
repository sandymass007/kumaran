<!doctype html>
<html><head><meta charset="utf-8"><title>Bookings</title>
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet"></head>
<body class="p-4">
  <div class="container">
    <h3>Bookings</h3>
    <table class="table mt-3">
      <thead><tr><th>Name</th><th>Email</th><th>Destination</th><th>Date</th><th>Booked At</th></tr></thead>
      <tbody>
        <% bookings.forEach(b => { %>
          <tr>
            <td><%= b.name %></td>
            <td><%= b.email %></td>
            <td><%= b.destination %></td>
            <td><%= b.date %></td>
            <td><%= b.createdAt %></td>
          </tr>
        <% }) %>
      </tbody>
    </table>
    <a class="btn btn-secondary" href="/">Home</a>
  </div>
</body></html>

